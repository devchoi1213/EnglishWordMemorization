{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0.0",
  "icons": {
    "16": "./icon.jpg",
    "48": "./icon.jpg",
    "128": "./icon.jpg"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }],
  "options_page": "options.html"
}